{
  "version": 2,
  "name": "agro-career-frontend",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "s-maxage=31536000,immutable"
      },
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://agro-career-backend.onrender.com",
    "REACT_APP_APP_NAME": "Agro Career",
    "REACT_APP_VERSION": "1.0.0"
  }
}
